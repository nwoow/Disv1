<%= simple_form_for(@dis_doseunit_convertion) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :from_doseunit_id %>
    <%= f.input :to_doseunit_id %>
    <%= f.input :conv_factor %>
    <%= f.input :status_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
