<%= simple_form_for(@dis_doseform_doseunit_link) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :doseform_id %>
    <%= f.input :doseunit_id %>
    <%= f.input :is_default %>
    <%= f.input :status_id %>
    <%= f.input :datasource_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
