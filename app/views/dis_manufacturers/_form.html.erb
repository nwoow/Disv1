<%= simple_form_for(@dis_manufacturer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :abbrev %>
    <%= f.input :address1 %>
    <%= f.input :address2 %>
    <%= f.input :city %>
    <%= f.input :state_id %>
    <%= f.input :pin %>
    <%= f.input :phone %>
    <%= f.input :phone2 %>
    <%= f.input :fax %>
    <%= f.input :email %>
    <%= f.input :website %>
    <%= f.input :mobilephone %>
    <%= f.input :status_id %>
    <%= f.input :release_status_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Save" %>
  </div>
<% end %>
