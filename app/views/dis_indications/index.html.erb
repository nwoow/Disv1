<p id="notice"><%= notice %></p>

<h1>Dis Indications</h1>

<table id="example" class="table table-bordered table-hover table-striped datatable" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Id</th>
      <th>Description</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @dis_indications.each do |dis_indication| %>
      <tr>
        <td><%= dis_indication.id %></td>
        <td><%= dis_indication.description %></td>
        <td><%= dis_indication.status_id %></td>
        <td><%= link_to 'Show', dis_indication %>,<%= link_to 'Edit', edit_dis_indication_path(dis_indication) %>,<%= link_to 'Destroy', dis_indication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dis Indication', new_dis_indication_path %>
