<%= simple_form_for(@dis_generic_monograph) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :generic_id %>
    <%= f.input :dose %>
    <%= f.input :contraindication %>
    <%= f.input :precaution %>
    <%= f.input :adverse_effect %>
    <%= f.input :storage %>
    <%= f.input :datasource_id %>
    <%= f.input :status_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
